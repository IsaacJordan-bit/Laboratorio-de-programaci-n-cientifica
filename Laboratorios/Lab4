      "cell_type": "code",
      "source": [
        "def clean_data(df):\n",
        "    df = df.drop_duplicates(subset='names').copy()\n",
        "    df = df[df['reading score'] > -1]\n",
        "    df = df[df['writing score'] > -1]\n",
        "    df = df[df['math score'] > -1]\n",
        "    df = df[df['reading score'] <= 100]\n",
        "    df = df[df['writing score'] <= 100]\n",
        "    df = df[df['math score'] <= 100]\n",
        "    return df"
      ],
      "metadata": {
        "id": "twq3pTyO3u_p"
      },
