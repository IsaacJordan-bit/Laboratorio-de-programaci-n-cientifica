    {
      "cell_type": "code",
      "source": [
        "def alumnos_promedio(df):\n",
        "    m = df['math score'].to_numpy()\n",
        "    r = df['reading score'].to_numpy()\n",
        "    w = df['writing score'].to_numpy()\n",
        "    df['mean score'] = np.round(np.mean(np.array([m,r,w]).T, axis=1), 2)\n",
        "    return df"
      ],
